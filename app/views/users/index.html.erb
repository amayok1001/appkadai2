<h2> Users</h2>
    <table>
        <tr class="booksline">
          <th> Image</th>
          <th> Name</td>
          <th></td>
        </tr>
        <% @users.each do |user| %>
          <tr>
			<td><%= attachment_image_tag user, :profile_image, :size =>"40x40", format:'jpeg', class: "pull-left profile-img", fallback: "no_image.png" %></td>
